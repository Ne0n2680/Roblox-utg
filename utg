local function IBUNJG_fake_script() -- UTGFrame.LocalScript 
	local script = Instance.new('LocalScript', UTGFrame)

	local Player = game.Players.LocalPlayer
	local Mouse = Player:GetMouse()
	
	Mouse.KeyDown:connect(function(key)
		if key == ';' then
			script.Parent.Visible = true
		end
	end)
	
end
coroutine.wrap(IBUNJG_fake_script)()
